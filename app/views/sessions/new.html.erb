<div id="form-container">

  <%= form_for :login do |f| %>
    <div>
      <%= f.text_field :email, placeholder: "Email Address" %>
      <%= f.password_field :password, placeholder:"Password" %>
    </div>
    <%= f.submit 'Log In' %>
    <p>Don't have an account? <a href="/signup">Signup</a></p>
   
  <% end %>
</div><!-- form-container -->

